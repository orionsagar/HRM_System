<style>
    .d-flex-row-center{
        display: flex;
        flex-direction:row;
        flex-wrap: wrap;
        justify-content: start;
        align-items: start;
        gap: 10px;
    }

    .FlipCardContainer {
        width: 100%;
    }

    .FlipCardWrapper{
        width: 95%;
    }
    .outer-card {
        width: 220px;
        height: 200px;
        perspective: 1000px;
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
    }

    .inner-card {
        position: relative;
        width: 100%;
        height: 100%;
        text-align: center;
        transition: transform 0.8s;
        transform-style: preserve-3d;
    }

    .outer-card:hover .inner-card {
        transform: rotateY(180deg);
    }

    .card-front,
    .card-back {
        position: absolute;
        width: 100%;
        height: 100%;
        backface-visibility: hidden;
        border-radius: 5px;

    }

    .card-front {
        background-color: #2c80e3;
        color: #ffff;
    }

    .card-back {
        background-color: #ffff;
        transform: rotateY(180deg);
        border-radius: 5px;
    }

    .card-front-content{
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }
    .card-front-content img{
        width: 50px;
        height: 50px;
        filter: invert(100%);
    }

    .moduleBackHeading{
        margin-top: 10px;
        text-align: center;
        width: 100%;
        height: 20px;
        color: #2c80e3;
        border-left: 2px solid #2c80e3;
        border: 5px;
    }

    .card-back-content{
        display: flex;
        flex-direction: column;
        justify-content: space-between;
        align-items: center;
    }
    .link-list{
        margin-top: 20px;
        display: flex;
        flex-direction: column;
        justify-content: start;
        align-items: start;
    }
    .link-list > ul{
            display: flex;
            flex-direction: column;
            justify-content: start;
            align-items: start;
    }

    .linkitem{
        width: 100%;
        height: 30px;
    }

    .BackCardButton{
        margin-top: 15px;
    }

    .BackCardButton a{
            background-color: #2c80e3;
            padding: 5px 10px;
            border-radius: 10px;
            color: #fff;
    }


</style>


