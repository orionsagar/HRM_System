@{
    ViewBag.Title = "Report";
}

@{
    string[] BCText = new string[] { "Report List" };
    string[] BCLink = new string[] { "report/index" };
    string createlink = "report/create";
    string linkname = "Add New";
    string IsAdd = ViewBag.IsAdd;

    PageHeaderProps pageHeader = new PageHeaderProps
    {
        PageTitle = "Report List",
        PageDesc = "Report List",
        BreadcrumbText = BCText,
        BreadcrumbLink = BCLink,
        PageCreateLinkName = linkname,
        PageCreateLink = createlink,
        IsAdd = IsAdd
    };

}

<div class="page-body">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    @await Component.InvokeAsync("PageHeader", pageHeader)
                </div>
                <div class="card-block">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="table-responsive">
                                <table id="dt-report" class="table table-striped table-bordered nowrap">
                                    <thead>
                                        <tr>

                                            <th>ReportId</th>
                                            <th>SL No</th>
                                            <th>Report Name</th>
                                            <th>Report Type</th>
                                            <th>Menu</th>
                                            <th>Report Link</th>
                                            <th>Id Name</th>
                                            <th>Class Name</th>
                                            <th>Report SL</th>
                                            <th></th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {

}