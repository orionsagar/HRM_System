@model IEnumerable<BillingMasterVM>
@{
    ViewData["Title"] = "Bill List";
}

@section PageName {
    Bill List
}
@section Breadcrumb {
    <a href="/department/index">
        <i class="fa fa-home"></i>
    </a>
    <span class="Divider">/</span>
    <span class="section activeCrum">Bill List</span>
}

<div class="page-body">
    <div class="row">
        <div class="col-sm-12">
            <div class="card">
                <div class="card-block">
                    <div class="table-responsive dt-responsive">
                        <table id="dt-bill" class="table table-striped table-bordered nowrap">
                            <thead>
                                <tr>
                                    <th>BillingMasterId</th>
                                    <th></th>
                                    <th>Bill No</th>
                                    <th>Bill Date</th>
                                    <th>Status</th>
                                    <th>Remarks</th>
                                    <th>Total Amount</th>
                                    <th>Pay Amount</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<input type="hidden" id="IsView" name="IsView" value="@ViewBag.IsView" />
<input type="hidden" id="IsAdd" name="IsAdd" value="@ViewBag.IsAdd" />
<input type="hidden" id="IsDelete" name="IsDelete" value="@ViewBag.IsDelete" />
<input type="hidden" id="IsEdit" name="IsEdit" value="@ViewBag.IsEdit" />




@section Scripts {

}