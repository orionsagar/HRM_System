                                    @*<th>Type In Bangla</th>*@
                                    <th>Description</th>
                                    @*<th>Is Govt</th>*@
                                    <th></th>
                                    <th></th>
                                </tr>
                            </thead>

                        </table>
                    </div>
                </div>
            </div>
            <!-- Ajax data source (Arrays) table end -->
        </div>
    </div>
</div>
