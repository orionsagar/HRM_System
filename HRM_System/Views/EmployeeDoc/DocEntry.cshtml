@inject IGlobalHelper _global;
@model EmpDocumentsVM;
@{
    var action = ViewBag.Action;
    if (action == "docentry")
    {
        ViewData["Title"] = "Document Info Add";
    }
    else
    {
        ViewData["Title"] = "Document Info Add";
    }
}
