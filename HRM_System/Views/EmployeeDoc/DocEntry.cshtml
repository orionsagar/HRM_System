            }
            if ((type_other == 'ref')) {
                if ($('#noData_ref').is(':visible')) {
                    $('#btnAdd_' + type_other).css("display", "inline-block");
                }
            }
            if ((type_other == 'lnk')) {
                if ($('#noData_lnk').is(':visible')) {
