            if ((type_other == 'spe')) {
                $('#noData_spe').css({
                    display: 'inline'
                });
                $('.spe_0').hide();
                $("#btnAdd_" + type_other).prop('disabled', false);
            }
            if ($('#commonForm_' + type_other + ' form').length == 0) {
                $("#noData_" + type_other).show();
            }
            $("#btnAdd_" + type_other).prop('disabled', false);
            //New Condition
            if ((type_other == 'lan')) {
                if ($('#noData_lan').is(':visible')) {
                    $('#btnAdd_' + type_other).css("display", "inline-block");
