@model UserProfileViewModel
@{
    ViewData["Title"] = "User Profile";
    Layout = "~/Views/Shared/_Main_Layout.cshtml";
}

<style>
    .user-title h2 {
        color: #000 !important;
    }

    .rowright {
        text-align: left !important;
    }

    .table td, .table th {
        text-align: left !important;
    }
</style>
<!--This Js and Other related are only used in this page-->
<!-- Page-body start -->
<!--profile cover start-->
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-block">
                <div class="cover-profile">
                    <div class="profile-bg-img">
                        @* <img class="profile-bg-img img-fluid" src="~/assets/images/user-profile/bg-img1.jpg" alt="bg-img"> *@
                        <div class="profile-bg-img img-fluid" alt="bg-img"></div>
                        <div class="user-info">
                            <div class="col-md-12">
                                <div class="media-left">
                                    <a href="#" class="profile-image">
                                        @{
            // if (!string.IsNullOrEmpty(Model.LogoPath))
                                            // {
                                            //     var imgPath = StaticData.HostPath + Model.Logo;
                                            //     <img class="user-img img-radius" src="@imgPath" width="100" alt="user-img" />
                                            // }
                                        }
                                        <img src="/assets/images/faq_man.png" class="img-radius" width="100" alt="user-img img-radius User-Profile-Image">
                                        @* <img class="user-img img-radius" src="~/assets/images/user-profile/user-img.jpg" alt="user-img"> *@
                                    </a>
                                </div>
                                <div class="media-body row">
                                    <div class="col-lg-12">
                                        <div class="user-title">
                                            <h2 class="text-black">@Model.FirstName @Model.LastName</h2>
                                            @{
                                                if (Model.UserStatus == "Active")
                                                {
                                                    <label class="label label-success">Active</label>
                                                }
                                                else if (Model.UserStatus == "Pending")
                                                {
                                                    <label class="label label-warning">Pending</label>
                                                }
                                                else
                                                {
                                                    <label class="label label-danger">Inactive</label>
                                                }
                                            }
                                            <br />
                                            <span class="text-black">Role: @Model.RoleName</span>
                                        </div>
                                    </div>
                                    <div>
                                        <div class="pull-right cover-btn">
                                            @* <button type="button" class="btn btn-primary m-r-10 m-b-10"><i class="icofont icofont-edit"></i> Edit</button> *@
                                            <button type="button" class="btn btn-primary m-b-10"><i class="icofont icofont-edit"></i> Edit</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<!--profile cover end-->
<div class="row">
    <div class="col-lg-12">
        <!-- tab header start -->
        <div class="tab-header card">
            <ul class="nav nav-tabs md-tabs tab-timeline" role="tablist" id="mytab">
                <li class="nav-item">
                    <a class="nav-link active" data-toggle="tab" href="#personal" role="tab">Personal Info</a>
                    <div class="slide"></div>
                </li>
                @{
                    if (Model.RoleType == "Org_Role")
                    {
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#OrgInfo" role="tab">Organisation Profile</a>
                            <div class="slide"></div>
                        </li>
                    }
                }

                @{
                    if (Model.RoleType != "Platform_Role")
                    {
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#binfo" role="tab">Client/Solicitor Detials</a>
                            <div class="slide"></div>
                        </li>
                    }
                }
                

                @* <li class="nav-item">
                <a class="nav-link" data-toggle="tab" href="#contacts" role="tab">Employees</a>
                <div class="slide"></div>
                </li> *@

                @* <li class="nav-item">
